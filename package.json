{
  "name": "oplayer",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=12",
    "pnpm": ">=7"
  },
  "author": "shiyiya",
  "keywords": [
    "oplayer",
    "video-player"
  ],
  "scripts": {
    "watch": "pnpm --parallel run start",
    "start": "pnpm run watch",
    "build:packs": "pnpm --filter=@oplayer/* run build && pnpm run clean:mmap",
    "clean:mmap": "rimraf packages/*/dist/*.min.js.map",
    "build:plugins": "pnpm --filter=@oplayer/plugins run build",
    "build:cache": "nx run-many --target=build --all --parallel && pnpm run clean:mmap",
    "build:affected": "nx Affected --target=build",
    "graph": "nx dep-graph",
    "clear": "rimraf 'packages/*/{dist,tsconfig.tsbuildinfo,node_modules,.rollup.cache}' && rimraf node_modules &&  rimraf docs",
    "changeset": "changeset",
    "release": "changeset publish",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.20.7",
    "@babel/plugin-transform-parameters": "^7.20.7",
    "@babel/plugin-transform-spread": "^7.20.7",
    "@babel/plugin-transform-template-literals": "^7.18.9",
    "@changesets/cli": "^2.26.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@types/node": "^18.11.17",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "nx": "^15.4.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "terser": "^5.16.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vite-plugin-banner": "^0.6.1",
    "vitest": "^0.25.8"
  }
}
